teo.point <- function(K, c0,t){
V0=eigen(K)
Xe=V0$vectors
lambda.0 <- V0$values
Xe1=solve(Xe)
init.rotate <- Xe1 %*% c0
### find lambda.t
lambda.t <- diag(exp(lambda.0*tt[t]))
### find system state at t
ct.t <- Xe %*% (lambda.t %*% init.rotate)
return(ct.t)
}# end teo.point
